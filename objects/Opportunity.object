    <validationRules>
        <fullName>Close_Date_Cant_be_Less_than_Today</fullName>
        <active>true</active>
        <errorConditionFormula>CloseDate &lt; Today()</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Close Date can not be earlier than Today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Lost_Reason</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(StageName, &quot;Closed Lost(0%)&quot;) &amp;&amp; ISBLANK( Lost_Reason__c)</errorConditionFormula>
        <errorDisplayField>Lost_Reason__c</errorDisplayField>
        <errorMessage>You need to provide a Closed Lost reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Next_Meeting_dat_Cant_be_Less_than_Today</fullName>
        <active>true</active>
        <errorConditionFormula>Next_Meeting_date__c &lt; Today()</errorConditionFormula>
        <errorDisplayField>Next_Meeting_date__c</errorDisplayField>
        <errorMessage>Next Meeting Date can not be earlier than Today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>On_Hold_Mandatory_fields</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( TEXT( Obstacle__c ) )
&amp;&amp; On_hold__c= True</errorConditionFormula>
        <errorDisplayField>Obstacle__c</errorDisplayField>
        <errorMessage>Obstacle value cannot be blank when  &quot;On-Hold&quot; checkbox is marked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>On_Hold_Mandatory_fields_Next_Step</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( TEXT(  Next_Step__c ) )
&amp;&amp; On_hold__c= True</errorConditionFormula>
        <errorDisplayField>Next_Step__c</errorDisplayField>
        <errorMessage>Next step value cannot be blanked when  &quot;On-Hold&quot; checkbox is marked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>On_Hold_Mandatory_fields_Obstacle_detail</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Obstacle__c,&quot;Customer deferred&quot; )&amp;&amp; NOT( ISBLANK(TEXT(Obstacle__c) ))&amp;&amp; ISBLANK(TEXT( Obstacle_Detail__c ) )</errorConditionFormula>
        <errorDisplayField>Obstacle_Detail__c</errorDisplayField>
        <errorMessage>Please enter Obstacle detail value for selected Obstacle</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>On_Hold_Stage_Change</fullName>
        <active>true</active>
        <errorConditionFormula>(ISCHANGED(StageName) || ISCHANGED( Probability ))
&amp;&amp; On_hold__c= True</errorConditionFormula>
        <errorMessage>Stage or Probability can not be changed when Opportunity is marked as On Hold.Please untick the On Hold checkbox to proceed to stage change</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>On_Hold_close_date</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(On_hold__c) &amp;&amp; On_hold__c = True &amp;&amp; NOT(ISCHANGED(CloseDate)) &amp;&amp; NOT(ISNEW() )</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Please adjust the close date as the opportunity is On Hold</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>On_hold_Next_meeting_date_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( TEXT(   Next_Meeting_date__c ) ) 
&amp;&amp; On_hold__c= True</errorConditionFormula>
        <errorDisplayField>Next_Meeting_date__c</errorDisplayField>
        <errorMessage>Next Meeting date cannot be blank when  &quot;On-Hold&quot; checkbox is marked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>On_hold_Reason_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Reason__c  ) 
&amp;&amp; On_hold__c= True</errorConditionFormula>
        <errorDisplayField>Reason__c</errorDisplayField>
        <errorMessage>On Hold reason cannot be blank when &quot;On-Hold&quot; checkbox is marked</errorMessage>
    </validationRules>
</CustomObject>
